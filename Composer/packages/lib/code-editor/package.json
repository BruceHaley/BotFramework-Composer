{
  "name": "@bfc/code-editor",
  "version": "0.0.0",
  "description": "Code editor component that is shared in composer packages",
  "main": "lib/index.js",
  "engines": {
    "node": "14.x"
  },
  "repository": "https://github.com/microsoft/BotFramework-Composer",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "yarn clean && yarn build:css && yarn build:ts",
    "build:ts": "tsc --build tsconfig.build.json",
    "build:css": "copyfiles --up 1 \"src/**/*.css\" \"src/**/*.scss\" lib",
    "clean": "rimraf lib demo/dist",
    "prepublishOnly": "npm run build",
    "start": "webpack-dev-server --config demo/webpack.config.demo.js",
    "build:demo": "webpack --config demo/webpack.config.demo.js",
    "test": "jest",
    "lint": "eslint --quiet ./src",
    "lint:fix": "yarn lint --fix",
    "lint:typecheck": "tsc --noEmit",
    "watch": "yarn build:ts --watch"
  },
  "peerDependencies": {
    "@bfc/built-in-functions": "*",
    "@bfc/indexers": "*",
    "@bfc/lg-languageserver": "*",
    "@bfc/shared": "*",
    "@bfc/ui-shared": "*",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "@bfc/indexers": "*",
    "@bfc/lg-languageserver": "*",
    "@bfc/shared": "*",
    "@botframework-composer/test-utils": "*",
    "@types/express": "4.16.1",
    "@types/lodash": "^4.14.146",
    "@types/node": "13.9.3",
    "@types/react": "16.9.23",
    "@types/react-dom": "16.9.5",
    "@types/vscode": "^1.40.0",
    "autoprefixer": "10.4.7",
    "copyfiles": "2.4.1",
    "css-loader": "6.7.1",
    "express": "4.18.1",
    "html-webpack-plugin": "5.5.0",
    "mini-css-extract-plugin": "2.6.1",
    "postcss-loader": "7.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rimraf": "3.0.2",
    "style-loader": "3.3.1",
    "ts-loader": "9.3.1",
    "url-loader": "4.1.1",
    "webpack": "5.73.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.9.2"
  },
  "dependencies": {
    "@emotion/react": "^11.1.3",
    "@emotion/styled": "^11.1.3",
    "@fluentui/react-icons": "^0.3.9",
    "@fluentui/theme": "2.5.0",
    "@monaco-editor/react": "^3.8.3",
    "json-schema-defaults": "0.4.0",
    "lodash": "4.17.21",
    "monaco-editor": "0.33.0",
    "monaco-languageclient": "2.0.2",
    "normalize-url": "7.0.3",
    "reconnecting-websocket": "4.4.0",
    "tslib": "2.4.0",
    "vscode-ws-jsonrpc": "1.0.1"
  }
}
