    "express": "4.18.1",
    "monaco-languageclient": "2.0.2",
    "normalize-url": "7.0.3",
    "reconnecting-websocket": "4.4.0",
    "tslib": "2.4.0",
    "vscode": "1.1.37",
    "vscode-json-languageservice": "5.0.0",
    "vscode-languageserver": "8.0.1",
    "vscode-ws-jsonrpc": "1.0.1",
    "ws": "8.8.0"
  },
  "devDependencies": {
    "@botframework-composer/test-utils": "*",
